name: <%= capability_statement.downcase %>
title: <%= title %>
description : <%= description %>
fhir_version: r4
default_test_set: ad_hoc_testing
test_sets:
  ad_hoc_testing:
    view: default
    tests:
      - name: SANER Profiles
        run_all: true
        sequences:<% sequences.each do |sequence| %>
        - <%=sequence[:class_name]%><% end %>

